{
    "name": "spatie/laravel-slack",
    "description": "Integrate Slack in a Laravel app",
    "keywords": [
        "spatie",
        "laravel-slack"
    ],
    "homepage": "https://github.com/spatie/laravel-slack",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench":  "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelSlack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LaravelSlack\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
